# EditorConfig is awesome:http://EditorConfig.org

# top-most EditorConfig file
root = true

# Baseline
[*]
charset = utf-8
indent_style = tab
trim_trailing_whitespace = true
max_line_length = 120
insert_final_newline = true

# MSBuild
[*.{csproj,proj,projitems,shproj,fsproj,target,props}]
indent_style = space
indent_size = 2

# XML config files
[*.{config,nuspec,resx}]
indent_style = space
indent_size = 2
